{
    "name": "eterlinden/elevate",
    "type": "library",
    "description": "A library in order to promote content through various channels.",
    "keywords": [
        "framework",
        "http",
        "rest",
        "web service",
        "curl",
        "client",
        "HTTP client"
    ],
    "homepage": "http://github.com/eterlinden/elevate",
    "license": "MIT",
    "authors": [
        {
            "name": "Ezra ter Linden",
            "email": "ezra.ter.linden@web.de"
        }
    ],
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "^6.2",
        "psr/log": "^1.0",
        "vlucas/phpdotenv": "^2.4",
        "php-di/php-di": "^5.4"
    },
    "require-dev": {
        "sami/sami": "^4.0",
        "squizlabs/php_codesniffer": "2.*",
        "phpunit/phpunit": "^5.7",
        "phpunit/phpunit-mock-objects": "^3.4",
        "phpunit/php-code-coverage": "^4.0"
    },
    "autoload": {
        "files": [
            "src/core/boostrap.php"
        ],
        "psr-4": {
            "Eterlinden\\Elevate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eterlinden\\Elevate\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
